<h1>Panel de control</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Email</th>
    </tr>
  </thead>

  <tbody>
    <% @users.each do |user| %>
    <tr>
      <td><%= user.name %></td>
      <td><%= user.email %></td>
    </tr>
    <% end %>
  </tbody>
</table>


<%= link_to 'Back', root_path, class: "btn btn-info btn-sm" %>

